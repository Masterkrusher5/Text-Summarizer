{
    "version": 2,
    "builds": [
      {
        "src": "./index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
    {
  "build": {
    "env": {
      "PIP_NO_CACHE_DIR": "false"
    }
  }
}

