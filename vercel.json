{
  "version": 2,
  "builds": [
    {
      "src": "./index.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.py"
    }
  ],
  "build": {
    "env": {
      "PIP_NO_CACHE_DIR": "false"
    }
  },
  "installCommand": "pip install --no-build-isolation -r requirements.txt"
}
